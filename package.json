{
  "name": "ui5-cc-excelupload",
  "version": "0.0.11",
  "description": "(add custom control description here)",
  "scripts": {
    "prepare": "node ./.husky/skip.js || husky install",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint -e",
    "prettier": "prettier --write .",
    "prettier:staged": "pretty-quick --staged --verbose",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "hooks:pre-commit": "npm-run-all --sequential prettier:staged lint:staged",
    "hooks:pre-push": "npm-run-all --sequential lint:commit",
    "version": "npm run changelog && git add --all",
    "version:publish": "git push && git push --tags",
    "build": "ui5 build --config=ui5-build.yaml --all --clean-dest --dest dist"
  },
  "keywords": [
    "ui5",
    "custom control",
    "notepad control"
  ],
  "author": "(you)",
  "repository": {
    "type": "git",
    "url": "cc.excelUpload.git"
  },
  "files": [
    "dist",
    "ui5.yaml"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@sap/ux-ui5-fe-mockserver-middleware": "1.6.5",
    "@sap/ux-ui5-tooling": "^1.7.3",
    "ui5-tooling-modules": "^0.7.0",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@ui5/cli": "^2.14.14",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-properties": "^0.2.0",
    "pretty-quick": "^3.1.3",
    "xlsx": "^0.18.5"
  }
}
