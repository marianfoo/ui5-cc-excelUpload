<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
	<Dialog title="Fehler beim Upload" resizable="true" state="Error" verticalScrolling="false" contentHeight="50%" contentWidth="50%">
        <content>
        <MessageView id="messageView" items="{errorData>/}">
            <MessageItem 
            title="{errorData>title}"
            type="{errorData>type}"
            counter="{errorData>counter}"
            subtitle="{errorData>subtitle}"
            ></MessageItem>
        </MessageView>
        </content>
        <endButton>
            <Button press=".onCloseErrorDialog" text="Ok"/>
        </endButton>    
	</Dialog>
</core:FragmentDefinition>